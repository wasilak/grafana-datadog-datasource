{
  "name": "wasilak-datadog-datasource",
  "version": "0.3.3",
  "description": "Datadog datasource",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "yarn watch",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "author": "Piotr Boruc",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/tsconfig": "^2.0.0",
    "@stylistic/eslint-plugin-ts": "~4.4.0",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "~6",
    "@testing-library/react": "~16",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.10",
    "@types/node": "~24.10.0",
    "@types/testing-library__jest-dom": "~6",
    "@typescript-eslint/eslint-plugin": "~8",
    "@typescript-eslint/parser": "~8",
    "copy-webpack-plugin": "~13.0.0",
    "css-loader": "~7",
    "eslint": "~9",
    "eslint-config-prettier": "~10",
    "eslint-plugin-deprecation": "~3",
    "eslint-plugin-jsdoc": "~61.4.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "~7.0.0",
    "eslint-webpack-plugin": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "~9",
    "glob": "~13.0.0",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "prettier": "~3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.94.2",
    "sass-loader": "~16",
    "style-loader": "~4",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node": "^10.9.2",
    "typescript": "~5",
    "webpack": "~5",
    "webpack-cli": "~6",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@datadog/datadog-api-client": "^1.21.0",
    "@emotion/css": "11.13.5",
    "@grafana/data": "^12.3.0",
    "@grafana/runtime": "^12.3.0",
    "@grafana/schema": "^12.3.0",
    "@grafana/ui": "^12.3.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  }
}
